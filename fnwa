local DarkraiX = loadstring(game:HttpGet("https://raw.githubusercontent.com/GamingScripter/Kavo-Ui/main/Darkrai%20Ui", true))()

local Library = DarkraiX:Window("FW Script Hub","V1",Enum.KeyCode.RightControl);

--[[
DarkraiX:Window(
1 = Name Of Your Ui Library (string)
2 = Game Name (You Can Keep This Empty To Get The Current Game Name!) (string)
3 = A Logo If You Have One! (string)
4 = Ui Library Toggle (I'll Not Prefer Touching It) (function)
);
]]

Tab1 = Library:Tab("FW Scripts")

--[[
Library:Tab(
1 = Your Tab Name! (string)
)
]]

Tab1:Button("case clicker auto clicker (no gui)",function()
    while wait() do
local args = {
    [1] = "Click",
    [3] = 51914
}

game:GetService("ReplicatedStorage").Remotes.RemoteEvent:FireServer(unpack(args))
end
end)

Tab1 = Library:Tab("other scripts")

Tab1:Button("pepsi swarm bee swarm",function()
loadstring(game:GetObjects("rbxassetid://4384103988")[0X1].Source)("Pepsi Swarm")
end)

Tab1:Button("universal farm jailbreak",function()
loadstring(game:HttpGet('https://raw.githubusercontent.com/BlitzIsKing/UniversalFarm/main/Loader/Regular'))()
end)

Tab1:Button("atomicx meepcity (OP)",function()
loadstring(game:HttpGet('https://raw.githubusercontent.com/yLegendzz/Scripts/main/Meepcity'))()
end)

Tab1 = Library:Tab("misc")

Tab1:Button("infinite yield",function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
end)

Tab1:Button("remote spy",function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/infyiff/backup/main/SimpleSpyV3/main.lua"))()
end)

Tab1:Button("audio logger",function()
loadstring(game:HttpGet(('https://raw.githubusercontent.com/infyiff/backup/main/audiologger.lua'),true))()
end)
